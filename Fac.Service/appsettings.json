{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"   
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://192.168.86.182:9400"
        }
      },
      {
        "Name": "Console"      
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "ApplicationName": "FacService API"
    }
  },
  "Authorization": {
    "Issuer": "XXXX",
    "Audience": "XXXX",
    "TokenExpires": 60
  },
  "PubSub": {
    "PubSubName": "FacService",
    "FacSendMibTopic": "FacSendMib",
    "FacCaseDecistionTopic": "FacCaseDecision",
    "FacCaseSubmissionTopic": "FacCaseSubmission"
  }
}
